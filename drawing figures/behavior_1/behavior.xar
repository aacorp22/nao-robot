		  GeneratedClass.__init__(self, False)
		except TypeError: # if NAOqi < 1.14
		  GeneratedClass.__init__( self )

	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("name")):
			self.output_1(p)
		elif(p == self.typeConversion("triangle")):
			self.output_2(p)
		elif(p == self.typeConversion("square")):
			self.output_3(p)
		elif(p == self.typeConversion("circle")):
			self.output_4(p)
		elif(p == self.typeConversion("picture")):
			self.output_5(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
